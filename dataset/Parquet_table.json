{
	"name": "Parquet_table",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"shemaName": {
				"type": "string"
			},
			"tableName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().tableName,'.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().shemaName,'/',dataset().tableName)",
					"type": "Expression"
				},
				"fileSystem": "bronzeyt"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}